    "# Predict in train\n",
    "predicted_prob_train = xgb_grid.best_estimator_.predict_proba(x_train)[:, 1]\n",
    "y_train_predicted = [1 if i >= optimal_proba_cutoff else 0 for i in predicted_prob_train]\n",
    "\n",
    "# Predict in validation\n",
